            <DropShadow />
         </effect></Button>
      <Button fx:id="redMonsterButton" layoutX="688.0" layoutY="175.0" mnemonicParsing="false" onAction="#redButton" prefHeight="250.0" prefWidth="250.0" style="-fx-background-radius: 100; -fx-background-color: none;">
