            <Menu mnemonicParsing="false" text="File">
               <items>
                  <MenuItem fx:id="close" mnemonicParsing="false" onAction="#close" text="Quit " />
               </items>
            </Menu>
         </menus>
         <effect>
            <DropShadow />
         </effect>
      </MenuBar>
      <Text fx:id="sidewalk" layoutX="945.0" layoutY="17.0" strokeType="OUTSIDE" strokeWidth="0.0" text="SideWalk" />
      <ImageView fitHeight="250.0" fitWidth="250.0" layoutX="689.0" layoutY="175.0">
         <image>
            <Image url="@RedMonster.png" />
         </image>
         <effect>
            <DropShadow />
         </effect>
      </ImageView>
      <ImageView fitHeight="250.0" fitWidth="250.0" layoutX="385.0" layoutY="175.0">
         <image>
            <Image url="@YellowMonster.png" />
         </image>
         <effect>
            <DropShadow />
         </effect>
      </ImageView>
      <ImageView fitHeight="250.0" fitWidth="250.0" layoutX="81.0" layoutY="177.0">
         <image>
            <Image url="@GreenMonster.png" />
         </image>
         <effect>
            <DropShadow />
         </effect>
      </ImageView>
      <Button fx:id="logButton" layoutX="467.0" layoutY="454.0" mnemonicParsing="false" onAction="#logBehavior" style="-fx-border-color: #8dc6ff; -fx-border-width: 3;" text="Log Behavior" />
      <Button layoutX="462.0" layoutY="554.0" mnemonicParsing="false" onAction="#backButton" style="-fx-background-color: None; -fx-text-fill: #8dc6ff;" text="Take Me Home" underline="true" />
      <Button fx:id="greenMonsterButton" layoutX="81.0" layoutY="175.0" mnemonicParsing="false" onAction="#greenButton" prefHeight="250.0" prefWidth="250.0" style="-fx-background-radius: 100; -fx-background-color: none;">
