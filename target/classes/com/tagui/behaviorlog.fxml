<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.tagui.behaviorLogController">
   <children>
      <ImageView fitHeight="250.0" fitWidth="250.0" layoutX="689.0" layoutY="175.0">
         <image>
            <Image url="@RedMonster.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="250.0" fitWidth="250.0" layoutX="385.0" layoutY="175.0">
         <image>
            <Image url="@YellowMonster.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="250.0" fitWidth="250.0" layoutX="81.0" layoutY="177.0">
         <image>
            <Image url="@GreenMonster.png" />
         </image>
      </ImageView>
      <Button fx:id="logButton" layoutX="467.0" layoutY="454.0" mnemonicParsing="false" onAction="#logBehavior" text="Log Behavior" />
      <Button layoutX="462.0" layoutY="554.0" mnemonicParsing="false" onAction="#backButton" style="-fx-background-color: None; -fx-text-fill: Blue;" text="Take Me Home" underline="true" />
      <Button fx:id="greenMonsterButton" layoutX="81.0" layoutY="175.0" mnemonicParsing="false" onAction="#greenButton" prefHeight="250.0" prefWidth="250.0" style="-fx-background-radius: 100; -fx-background-color: none;" />
      <Button fx:id="redMonsterButton" layoutX="688.0" layoutY="175.0" mnemonicParsing="false" onAction="#redButton" prefHeight="250.0" prefWidth="250.0" style="-fx-background-radius: 100; -fx-background-color: none;" />
      <Button fx:id="yellowMonsterButton" layoutX="385.0" layoutY="177.0" mnemonicParsing="false" onAction="#yellowButton" prefHeight="250.0" prefWidth="250.0" style="-fx-background-radius: 100; -fx-background-color: none;" />
   </children>
</AnchorPane>
