            <DropShadow />
         </effect></Button>
      <Button fx:id="yellowMonsterButton" layoutX="385.0" layoutY="177.0" mnemonicParsing="false" onAction="#yellowButton" prefHeight="250.0" prefWidth="250.0" style="-fx-background-radius: 100; -fx-background-color: none;">
