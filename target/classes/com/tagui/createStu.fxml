<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="442.0" prefWidth="561.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.tagui.creStuController">
    <children>
        <Label fx:id="myLabel" layoutX="36.0" layoutY="14.0" prefHeight="81.0" prefWidth="490.0" text="Enter the fields to create a new student" textAlignment="CENTER">
            <font>
                <Font size="28.0" />
            </font>
        </Label>
        <TextField fx:id="nameField" layoutX="122.0" layoutY="115.0" prefHeight="26.0" prefWidth="318.0" promptText="Student's Name" />
        <TextField fx:id="gpaField" layoutX="121.0" layoutY="156.0" prefHeight="26.0" prefWidth="318.0" promptText="Student's GPA" />
        <TextField fx:id="behField" layoutX="121.0" layoutY="194.0" prefHeight="26.0" prefWidth="318.0" promptText="Student's Behavior" />
        <TextField fx:id="seatField" layoutX="121.0" layoutY="233.0" prefHeight="26.0" prefWidth="318.0" promptText="Student's Seat" />
        <TextField fx:id="attField" layoutX="121.0" layoutY="273.0" prefHeight="26.0" prefWidth="318.0" promptText="Student's Attendance" />
        <Button fx:id="createButton" layoutX="122.0" layoutY="315.0" mnemonicParsing="false" onAction="#create" prefHeight="58.0" prefWidth="103.0" text="Create" />
        <Button fx:id="cancelButton" layoutX="336.0" layoutY="315.0" mnemonicParsing="false" onAction="#cancel" prefHeight="58.0" prefWidth="103.0" text="Cancel" />
    </children>
</AnchorPane>
